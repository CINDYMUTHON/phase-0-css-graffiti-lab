{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "img/icon.png",
      "default_popup": "popup.html",
      "default_title": "MergeMail"
   },
   "content_scripts": [ {
      "css": [ "css/content/style.css" ],
      "js": [ "js/content/jquery.js", "js/content/inboxsdk.js", "js/content.js" ],
      "matches": [ "*://mail.google.com/*" ]
   } ],
   "description": "Mail merge for Gmail",
   "externally_connectable": {
      "matches": [ "*://www.mergemail.co/*" ]
   },
   "homepage_url": "https://www.mergemail.co/",
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "48": "img/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "MergeMail",
   "permissions": [ "http://mail.google.com/", "https://mail.google.com/", "https://www.mergemail.co/*" ],
   "short_name": "MergeMail",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.5.0",
   "web_accessible_resources": [ "img/*", "js/vendor/jquery-2.0.3.min.js", "frame.html", "frame_loading.html" ]
}
